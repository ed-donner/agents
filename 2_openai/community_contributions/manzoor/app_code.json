"{\n  \"file_name\": \"smart-inventory-manager\",\n  \"type\": \"directory\",\n  \"path\": \"./smart-inventory-manager\",\n  \"content\": null,\n  \"children\": [\n    {\n      \"file_name\": \"package.json\",\n      \"type\": \"file\",\n      \"path\": \"smart-inventory-manager/package.json\",\n      \"content\": \"{\\\"name\\\":\\\"smart-inventory-manager\\\",\\\"version\\\":\\\"0.1.0\\\",\\\"private\\\":true,\\\"dependencies\\\":{\\\"react\\\":\\\"^18.2.0\\\",\\\"react-dom\\\":\\\"^18.2.0\\\",\\\"react-router-dom\\\":\\\"^6.15.0\\\",\\\"axios\\\":\\\"^1.3.6\\\",\\\"typescript\\\":\\\"^5.1.6\\\"},\\\"scripts\\\":{\\\"start\\\":\\\"vite\\\",\\\"build\\\":\\\"vite build\\\",\\\"test\\\":\\\"echo 'No tests'\\\"}}\"\n    },\n    {\n      \"file_name\": \"public\",\n      \"type\": \"directory\",\n      \"path\": \"smart-inventory-manager/public\",\n      \"content\": null,\n      \"children\": [\n        {\n          \"file_name\": \"index.html\",\n          \"type\": \"file\",\n          \"path\": \"smart-inventory-manager/public/index.html\",\n          \"content\": \"<!doctype html>\\n<html lang=\\\"en\\\">\\n<head>\\n  <meta charset=\\\"UTF-8\\\" />\\n  <meta name=\\\"viewport\\\" content=\\\"width=device-width, initial-scale=1\\\" />\\n  <title>Smart Inventory Manager</title>\\n</head>\\n<body>\\n  <div id=\\\"root\\\"></div>\\n</body>\\n</html>\"\n        }\n      ]\n    },\n    {\n      \"file_name\": \"src\",\n      \"type\": \"directory\",\n      \"path\": \"smart-inventory-manager/src\",\n      \"content\": null,\n      \"children\": [\n        {\n          \"file_name\": \"main.tsx\",\n          \"type\": \"file\",\n          \"path\": \"smart-inventory-manager/src/main.tsx\",\n          \"content\": \"import React from 'react';\\nimport { createRoot } from 'react-dom/client';\\nimport App from './App';\\nimport './styles.css';\\n\\nconst root = document.getElementById('root');\\nif (root) {\\n  createRoot(root).render(\\n    <React.StrictMode>\\n      <App />\\n    </React.StrictMode>\\n  );\\n}\\n\"\n        },\n        {\n          \"file_name\": \"App.tsx\",\n          \"type\": \"file\",\n          \"path\": \"smart-inventory-manager/src/App.tsx\",\n          \"content\": \"import React from 'react';\\nimport { BrowserRouter, Routes, Route, Navigate } from 'react-router-dom';\\nimport DashboardPage from './pages/DashboardPage';\\nimport InventoryPage from './pages/InventoryPage';\\nimport ReorderAlertsPage from './pages/ReorderAlertsPage';\\nimport SuppliersPage from './pages/SuppliersPage';\\nimport AnalyticsPage from './pages/AnalyticsPage';\\nimport SettingsPage from './pages/SettingsPage';\\nimport UserProfilePage from './pages/UserProfilePage';\\n\\nexport const App: React.FC = () => {\\n  return (\\n    <BrowserRouter>\\n      <Routes>\\n        <Route path=\\\"/\\\" element={<DashboardPage />} />\\n        <Route path=\\\"/inventory\\\" element={<InventoryPage />} />\\n        <Route path=\\\"/alerts\\\" element={<ReorderAlertsPage />} />\\n        <Route path=\\\"/suppliers\\\" element={<SuppliersPage />} />\\n        <Route path=\\\"/analytics\\\" element={<AnalyticsPage />} />\\n        <Route path=\\\"/settings\\\" element={<SettingsPage />} />\\n        <Route path=\\\"/profile\\\" element={<UserProfilePage />} />\\n        <Route path=\\\"*\\\" element={<Navigate to=\\\"/\\\" />} />\\n      </Routes>\\n    </BrowserRouter>\\n  );\\n};\\n\\nexport default App;\"\n        },\n        {\n          \"file_name\": \"pages/DashboardPage.tsx\",\n          \"type\": \"file\",\n          \"path\": \"smart-inventory-manager/src/pages/DashboardPage.tsx\",\n          \"content\": \"import React from 'react';\\nimport ForecastChart from '../components/ForecastChart';\\n\\ntype Point = { date: string; forecastQuantity: number; };\\nconst DashboardPage: React.FC = () => {\\n  const sample: Point[] = [\\n    { date: '2025-01-01', forecastQuantity: 120 },\\n    { date: '2025-02-01', forecastQuantity: 135 },\\n    { date: '2025-03-01', forecastQuantity: 150 },\\n    { date: '2025-04-01', forecastQuantity: 170 }\\n  ];\\n  return (\\n    <div>\\n      <h1>Dashboard</h1>\\n      <ForecastChart data={sample} />\\n    </div>\\n  );\\n};\\nexport default DashboardPage;\"\n        },\n        {\n          \"file_name\": \"pages/InventoryPage.tsx\",\n          \"type\": \"file\",\n          \"path\": \"smart-inventory-manager/src/pages/InventoryPage.tsx\",\n          \"content\": \"import React from 'react';\\nimport InventoryList from '../components/InventoryList';\\nimport { Product, InventoryItem } from '../types';\\n\\nconst InventoryPage: React.FC = () => {\\n  const products: Product[] = [\\n    { id: 1, name: 'Widget A', sku: 'WGT-A', priceCost: 5, priceSelling: 10 },\\n    { id: 2, name: 'Widget B', sku: 'WGT-B', priceCost: 6, priceSelling: 12 }\\n  ];\\n  const inventory: InventoryItem[] = [\\n    { id: 1, productId: 1, warehouseId: 1, quantity: 120 },\\n    { id: 2, productId: 2, warehouseId: 1, quantity: 50 }\\n  ];\\n  return (\\n    <div>\\n      <h1>Inventory</h1>\\n      <InventoryList items={inventory} products={products} />\\n    </div>\\n  );\\n};\\nexport default InventoryPage;\"\n        },\n        {\n          \"file_name\": \"pages/ReorderAlertsPage.tsx\",\n          \"type\": \"file\",\n          \"path\": \"smart-inventory-manager/src/pages/ReorderAlertsPage.tsx\",\n          \"content\": \"import React from 'react';\\nconst ReorderAlertsPage: React.FC = () => {\\n  return (<div><h1>Reorder Alerts</h1></div>);\\n};\\nexport default ReorderAlertsPage;\"\n        },\n        {\n          \"file_name\": \"pages/SuppliersPage.tsx\",\n          \"type\": \"file\",\n          \"path\": \"smart-inventory-manager/src/pages/SuppliersPage.tsx\",\n          \"content\": \"import React from 'react';\\nconst SuppliersPage: React.FC = () => {\\n  return (<div><h1>Suppliers</h1></div>);\\n};\\nexport default SuppliersPage;\"\n        },\n        {\n          \"file_name\": \"pages/AnalyticsPage.tsx\",\n          \"type\": \"file\",\n          \"path\": \"smart-inventory-manager/src/pages/AnalyticsPage.tsx\",\n          \"content\": \"import React from 'react';\\nconst AnalyticsPage: React.FC = () => {\\n  return (<div><h1>Analytics</h1></div>);\\n};\\nexport default AnalyticsPage;\"\n        },\n        {\n          \"file_name\": \"pages/SettingsPage.tsx\",\n          \"type\": \"file\",\n          \"path\": \"smart-inventory-manager/src/pages/SettingsPage.tsx\",\n          \"content\": \"import React from 'react';\\nconst SettingsPage: React.FC = () => {\\n  return (<div><h1>Settings</h1></div>);\\n};\\nexport default SettingsPage;\"\n        },\n        {\n          \"file_name\": \"pages/UserProfilePage.tsx\",\n          \"type\": \"file\",\n          \"path\": \"smart-inventory-manager/src/pages/UserProfilePage.tsx\",\n          \"content\": \"import React from 'react';\\nconst UserProfilePage: React.FC = () => {\\n  return (<div><h1>User Profile</h1></div>);\\n};\\nexport default UserProfilePage;\"\n        },\n        {\n          \"file_name\": \"components/ForecastChart.tsx\",\n          \"type\": \"file\",\n          \"path\": \"smart-inventory-manager/src/components/ForecastChart.tsx\",\n          \"content\": \"import React from 'react';\\ntype Point = { date: string; forecastQuantity: number; };\\ninterface Props { data: Point[]; }\\nconst ForecastChart: React.FC<Props> = ({ data }) => {\\n  const max = Math.max(...data.map(d => d.forecastQuantity), 1);\\n  return (\\n    <svg width=\\\"100%\\\" height=\\\"150\\\" viewBox=\\\"0 0 \\\"+ (data.length*40) +\\\" 150\\\" style={ { display: 'block' } }>\\n      {data.map((p, i) => (\\n        <rect key={i} x={i * 40 + 10} y={150 - (p.forecastQuantity / max) * 120} width={20} height={(p.forecastQuantity / max) * 120} fill=\\\"#4caf50\\\" />\\n      ))}\\n    </svg>\\n  );\\n};\\nexport default ForecastChart;\"\n        },\n        {\n          \"file_name\": \"components/InventoryList.tsx\",\n          \"type\": \"file\",\n          \"path\": \"smart-inventory-manager/src/components/InventoryList.tsx\",\n          \"content\": \"import React from 'react';\\nimport { InventoryItem, Product } from '../types';\\ninterface Props { items: InventoryItem[]; products: Product[]; }\\nconst InventoryList: React.FC<Props> = ({ items, products }) => {\\n  const map = new Map<number, Product>();\\n  products.forEach(p => map.set(p.id, p));\\n  return (\\n    <ul style={{ listStyle: 'none', padding: 0 }}>\\n      {items.map(it => { const p = map.get(it.productId); return (\\n        <li key={it.id} style={{ border: '1px solid #ddd', padding: 8, marginBottom: 8 }}>\\n          <strong>{p?.name ?? 'Unknown'}</strong> - Qty: {it.quantity}\\n        </li>\\n      ); })}\\n    </ul>\\n  );\\n};\\nexport default InventoryList;\"\n        },\n        {\n          \"file_name\": \"components/ProductCard.tsx\",\n          \"type\": \"file\",\n          \"path\": \"smart-inventory-manager/src/components/ProductCard.tsx\",\n          \"content\": \"import React from 'react';\\nimport { Product } from '../types';\\ninterface Props { product: Product; }\\nconst ProductCard: React.FC<Props> = ({ product }) => (\\n  <div style={{ border: '1px solid #ddd', padding: 12, borderRadius: 8 }}>\\n    <h3>{product.name}</h3>\\n    <p>SKU: {product.sku}</p>\\n    <p>Cost: ${product.priceCost}</p>\\n    <p>Sell: ${product.priceSelling}</p>\\n  </div>\\n);\\nexport default ProductCard;\"\n        },\n        {\n          \"file_name\": \"types.ts\",\n          \"type\": \"file\",\n          \"path\": \"smart-inventory-manager/src/types.ts\",\n          \"content\": \"export type Product = { id: number; name: string; sku: string; description?: string; unit?: string; priceCost: number; priceSelling: number; supplierId?: number; };\\nexport type InventoryItem = { id: number; productId: number; warehouseId: number; quantity: number; minThreshold?: number; maxThreshold?: number; };\"\n        },\n        {\n          \"file_name\": \"styles.css\",\n          \"type\": \"file\",\n          \"path\": \"smart-inventory-manager/src/styles.css\",\n          \"content\": \"body { font-family: Arial, sans-serif; padding: 20px; }\"\n        },\n        {\n          \"file_name\": \"utils/formatters.ts\",\n          \"type\": \"file\",\n          \"path\": \"smart-inventory-manager/src/utils/formatters.ts\",\n          \"content\": \"export const currency = (n: number) => `$${n.toFixed(2)}`;\"\n        },\n        {\n          \"file_name\": \"hooks/useWebSocket.ts\",\n          \"type\": \"file\",\n          \"path\": \"smart-inventory-manager/src/hooks/useWebSocket.ts\",\n          \"content\": \"import { useEffect, useRef } from 'react';\\nexport const useWebSocket = (url: string, onMessage: (ev: MessageEvent) => void) => {\\n  const ws = useRef<WebSocket | null>(null);\\n  useEffect(() => {\\n    ws.current = new WebSocket(url);\\n    ws.current.onmessage = onMessage;\\n    return () => { ws.current?.close(); };\\n  }, [url]);\\n};\"\n        }\n      ]\n    }\n  ]\n}\n"